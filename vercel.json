{
  "version": 2,
  "builds": [
    {
      "src": "server-web.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server-web.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server-web.js": {
      "maxDuration": 30
    }
  }
} 